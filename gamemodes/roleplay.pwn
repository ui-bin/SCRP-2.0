	if (sscanf(params, "us[16]S()[32]", userid, type, amount))
 	{
	 	SendSyntaxMessage(playerid, "/setplayer [playerid/name] [name]");
	 	SendClientMessage(playerid, COLOR_ORANGE, "[NAMES]:{FFFFFF} gender, birthdate, origin, bank, savings, hunger, thirst, playinghours.");
