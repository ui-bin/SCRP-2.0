CMD:disguise(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "You don't have permission to use this command.");

	new type[12], stringz[24];
	if (sscanf(params, "s[12]S()[24]", type, stringz))
		return SendSyntaxMessage(playerid, "/disguise [as/clear]");

	if (!strcmp(type, "as", true))
	{
		new targetid;
		if (sscanf(stringz, "u", targetid))
			return SendSyntaxMessage(playerid, "/disguise as [playerid]");

		if (PlayerData[playerid][pDisguise])
			return SendErrorMessage(playerid, "You are already been disguised as %s", PlayerData[playerid][pDisguiseID]);

		if (targetid == playerid)
			return SendErrorMessage(playerid, "You cannot disguise as yourself.");

		if (!IsPlayerConnected(targetid) || targetid == INVALID_PLAYER_ID)
			return SendErrorMessage(playerid, "Invalid playerid");
		
		foreach (new i : Player) if (PlayerData[i][pDisguiseID] == targetid) {
			return SendErrorMessage(playerid, "Sorry, someone has disguised as %s", ReturnName(targetid));
		}

		SetPlayerInterior(playerid, GetPlayerInterior(targetid));
		SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(targetid));

		TogglePlayerSpectating(playerid, true);

		if (IsPlayerInAnyVehicle(targetid))
	    	PlayerSpectateVehicle(playerid, GetPlayerVehicleID(targetid));
		else
			PlayerSpectatePlayer(playerid, targetid);
		
		SetPVarInt(playerid, "current_color", GetPlayerColor(playerid));
		SetPlayerColor(playerid, 0x00000000);

		PlayerData[playerid][pDisguise] = 1;
		PlayerData[playerid][pDisguiseID] = targetid;
		SendServerMessage(playerid, "You're now disguised as %s", ReturnName(targetid));
	}
	else if(!strcmp(type, "clear", true))
	{
		if (!PlayerData[playerid][pDisguise])
			return SendErrorMessage(playerid, "You are not disguised as someone");

		PlayerSpectatePlayer(playerid, INVALID_PLAYER_ID);
		PlayerSpectateVehicle(playerid, INVALID_VEHICLE_ID);

		TogglePlayerSpectating(playerid, false);
		SetPlayerColor(playerid, GetPVarInt(playerid, "current_color"));

		SetSpawnInfo(playerid, 0, PlayerData[playerid][pSkin], PlayerData[playerid][pPos][0], PlayerData[playerid][pPos][1], PlayerData[playerid][pPos][2], PlayerData[playerid][pPos][3], 0, 0, 0, 0, 0, 0);
		DeletePVar(playerid, "current_color");
		
		SendServerMessage(playerid, "You're now not disguised as %s", ReturnName(PlayerData[playerid][pDisguiseID]));
		PlayerData[playerid][pDisguise] = 0;
		PlayerData[playerid][pDisguiseID] = INVALID_PLAYER_ID;
	}
	return 1;
}

CMD:ask(playerid, asked_question[])
