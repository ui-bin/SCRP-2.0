			SendVehicleMessage(GetPlayerVehicleID(PlayerData[playerid][pDisguiseID]), 0xBBFFEEFF, "[Vehicle] %s says: %s", ReturnName(PlayerData[playerid][pDisguiseID], 0), text);

        else if (IsPlayerInAnyVehicle(playerid) && IsWindowedVehicle(GetPlayerVehicleID(playerid)) && !CoreVehicles[GetPlayerVehicleID(playerid)][vehWindowsDown])
			SendVehicleMessage(GetPlayerVehicleID(playerid), 0xBBFFEEFF, "[Vehicle] %s says: %s", ReturnName(playerid, 0), text);

		else
		{
