ShowWarnings(playerid, targetid)
{
	new
		Cache:_gett,
		count = 0,
		reason[128],
		string[1024];

	mysql_format(g_iHandle, string, sizeof(string), "SELECT * FROM `Warnings` WHERE `Name` = '%e'", ReturnName(targetid, 1));
	_gett = mysql_query(g_iHandle, string);

	string[0] = EOS;
	for(new i = 0, rows = cache_num_rows(); i < rows; i++)
	{
		cache_get_value_name(i, "Reason", reason, 128);

		format(string, sizeof(string), "%s\n[WARN] %s", string, reason);
		count++;
	}

	if(!count)
		return SendClientMessageEx(targetid, COLOR_CLIENT, "%s don't have any warns!.", ReturnName(targetid));

	Dialog_Show(playerid, ShowOnly, DIALOG_STYLE_LIST, "List Warnings", string, "Okay", "Close");
	cache_delete(_gett);
	return 1;
}

