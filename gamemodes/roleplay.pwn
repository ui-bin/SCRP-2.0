		SetPlayerProgressBarValue(i, PlayerData[i][pHungerBar], PlayerData[i][pHunger]);
		SetPlayerProgressBarValue(i, PlayerData[i][pThirstBar], PlayerData[i][pThirst]);
		SetPlayerProgressBarValue(i, PlayerData[i][pBladderBar], PlayerData[i][pBladder]);

		SetPlayerProgressBarColour(i, PlayerData[i][pHungerBar], ReturnBarColour(PlayerData[i][pHunger]));
		SetPlayerProgressBarColour(i, PlayerData[i][pThirstBar], ReturnBarColour(PlayerData[i][pThirst]));
		SetPlayerProgressBarColour(i, PlayerData[i][pBladderBar], ReturnBarColour(PlayerData[i][pBladder]));
