FormatNumber(number, prefix[] = "$")
{
	static
		value[33],
		length;

	format(value, sizeof(value), "%d", (number < 0) ? (-number) : (number));

	if ((length = strlen(value)) > 3)
	{
		for (new i = length, l = 0; --i >= 0; l ++) {
		    if ((l > 0) && (l % 3 == 0)) strins(value, ",", i + 1);
		}
	}
	if (prefix[0] != 0)
	    strins(value, prefix, 0);

	if (number < 0)
		strins(value, "-", 0);

	if (number < 100)
		strins(value, "0.", 1);	

	if (number > 99)
		strins(value, ".", length-2);	

	return value;
}

