CMD:b(playerid, params[])
{
	if (isnull(params))
	    return SendSyntaxMessage(playerid, "/b [local OOC]");
		
	if (strlen(params) > 64)
	{
	    if (PlayerData[playerid][pStaffDuty])
	    {
	        SendNearbyMessage(playerid, 20.0, COLOR_WHITE, "{%s}%s{FFFFFF} [%d]: (( %.64s", (PlayerData[playerid][pAdmin]) ? ("E9051A") : ("33EE33"), ReturnName(playerid, 0), playerid, params);
	    	SendNearbyMessage(playerid, 20.0, COLOR_WHITE, "...%s ))", params[64]);
	        return 1;
		}
		
		if (PlayerData[playerid][pDisguise])
		{
			SendNearbyMessage(PlayerData[playerid][pDisguiseID], 20.0, COLOR_WHITE, "%s [%d]: (( %.64s", ReturnName(PlayerData[playerid][pDisguiseID], 0), PlayerData[playerid][pDisguiseID], params);
	   		SendNearbyMessage(PlayerData[playerid][pDisguiseID], 20.0, COLOR_WHITE, "...%s ))", params[64]);
			return 1;
		}
