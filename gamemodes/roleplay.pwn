		GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~r~Spraying vehicle...", 3000, 3);
		SetPreciseTimer("ResprayCar", 3000, false, "ddd", playerid, vehicleid, modelid);
