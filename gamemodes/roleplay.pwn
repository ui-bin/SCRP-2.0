GetFishFreeSlot(playerid)
{
	for (new i = 0; i != MAX_TOTAL_FISH; i ++)
	{
		if (PlayerData[playerid][pFish][i] == 0)
			return i;
	}
	return -1;
}

GetTotalFishWeight(playerid)
{
	new valueu;
	for (new i = 0; i != MAX_TOTAL_FISH; i ++)
		valueu += PlayerData[playerid][pFishWeight][i];	

	return ((valueu != 0) ? (valueu / 12) : (valueu));
}

ClearFish(playerid)
{
	for (new i = 0; i != MAX_TOTAL_FISH; i ++)
	{
		PlayerData[playerid][pFish][i] = 0;
		PlayerData[playerid][pFishWeight][i] = 0;
	}
	return 1;
}

FloatNumber(int)
{
	new strinsq[12], pros;

	if (int > 999) 
		pros = 2;
	else 
		pros = 1;

	if(int != 0)
	{
		format(strinsq, sizeof(strinsq), "%d", int);
		strins(strinsq, ".", pros);
	}
	else strinsq = "0.0";
	return strinsq;
}
