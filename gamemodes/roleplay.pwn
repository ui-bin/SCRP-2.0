		{
			SendClientMessage(playerid, COLOR_GREY, "ERROR: You are not close enough to this vehicle anymore.");
			return 1;
		}

		g_Bomb_Vehicles[vehicleid][bv_i_ArmedType] = bomb_type;
		g_Bomb_Vehicles[vehicleid][bv_i_ExplosionTimer] = bomb_timer;
