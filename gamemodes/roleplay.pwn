ReturnDate()
{
	static
	    date[36];

	getdate(date[2], date[1], date[0]);
	gettime(date[3], date[4], date[5]);

	format(date, sizeof(date), "%02d/%02d/%d, %02d:%02d", date[0], date[1], date[2], date[3], date[4]);
	return date;
}

IsLeapYear(year)
{
	if(year % 4 == 0) return 1;
	else return 0;
}

FormatSQLDate(date[])
{
	new output[24], year, month, day, hour, minute;

	// Extract time from date, so we can get the time EXACTLY!
	strmid(output, date, 11, 16);

	// Check with sscanf and put the number on variable.
	sscanf(date, "p<->iii", year, month, day);
	sscanf(output, "p<:>ii", hour, minute);

	// Rebuild correct date and replace output with new one.
	format(output, sizeof(output), "%02d/%02d/%02d, %02d:%02d", day, month, year, hour, minute);
	return output;
}

ConvertTimestamp(Timestampu, HourGMT = 7, MinuteGMT = 0)
{
	new 
		output[42],
		tmp = 2,
		year = 1970,
		month = 1,
		day,
		hour,
		minute;
		//second;

	Timestampu -= 172800; // Delete two days from the current timestamp. This is necessary, because the timestamp retrieved using gettime() includes two too many days.
	for(;;)
	{
		if(Timestampu >= 31536000)
		{
			year ++;
			Timestampu -= 31536000;
			tmp ++;
			if(tmp == 4)
			{
				if(Timestampu >= 31622400)
				{
					tmp = 0;
					year ++;
					Timestampu -= 31622400;
				}
				else break;
			}
		}
		else break;
	}		
	for(new i = 0; i < 12; i ++)
	{
		if(Timestampu >= MonthTimes[i][2 + IsLeapYear(year)])
		{
			month ++;
			Timestampu -= MonthTimes[i][2 + IsLeapYear(year)];
		}
		else break;
	}
	day = 1 + (Timestampu / 86400);
	Timestampu %= 86400;
	hour = HourGMT + (Timestampu / 3600);
	Timestampu %= 3600;
	minute = MinuteGMT + (Timestampu / 60);
	//second = (Timestampu % 60);
	if(minute > 59)
	{
		minute = 0;
		hour ++;
	}
	if(hour > 23)
	{
		hour -= 24;
		day ++;
	}	
	if(day > MonthTimes[month][IsLeapYear(year)])
	{
		day = 1;
		month ++;
	}
	if(month > 12)
	{
		month = 1;
		year ++;
	}
	format(output, sizeof(output), "%02d/%02d/%02d, %02d:%02d", day, month, year, hour, minute);
	return output;
}


