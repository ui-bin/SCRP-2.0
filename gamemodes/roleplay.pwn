CMD:warn(playerid, params[])
{
	static
	    userid,
		reason[64],
		output[64],
		par[160];

    if (!PlayerData[playerid][pAdmin])
	    return SendErrorMessage(playerid, "You don't have permission to use this command.");

	if (sscanf(params, "us[64]", userid, reason))
	    return SendSyntaxMessage(playerid, "/warn [playerid/name] [reason]");

    if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "You have specified an invalid player.");

	if (PlayerData[userid][pAdmin] > PlayerData[playerid][pAdmin])
	    return SendErrorMessage(playerid, "The specified player has higher authority.");

	mysql_format(g_iHandle, par, sizeof(par), "INSERT INTO `Warnings` (`Name`, `Warning By`, `Reason`) VALUES ('%s', '%s', '%s')",
		SQL_ReturnEscaped(ReturnName(userid)),
		SQL_ReturnEscaped(ReturnName(playerid)),
		SQL_ReturnEscaped(reason)
	);
	mysql_tquery(g_iHandle, par);

	switch(++ PlayerData[playerid][pWarnings])
	{
		case 5, 10:
		{
			format(output, sizeof(output), "%d 25 %d Warnings", userid, PlayerData[playerid][pWarnings]);
			callcmd::ajail(playerid, output);

			SendAdminAction(userid, "You've been warned by %s for %s", ReturnStaffName(playerid), reason);
			SendStaffAlert(COLOR_LIGHTRED, "[ADMIN] %s was been warned by %s, reason: %s", ReturnName(userid, 0), ReturnStaffName(playerid), reason);
			Log_Write("logs/warn_log.txt", "[%s] %s has warned %s for %s.", ReturnDate(), ReturnStaffName(playerid), ReturnName(userid, 0), reason);
		}
		case 15:
		{
			format(output, sizeof(output), "%d 1 %d Warnings", userid, PlayerData[playerid][pWarnings]);
			callcmd::tempban(playerid, output);

			SendAdminAction(userid, "You've been warned by %s for %s", ReturnStaffName(playerid), reason);
			SendStaffAlert(COLOR_LIGHTRED, "[ADMIN] %s was been warned by %s, reason: %s", ReturnName(userid, 0), ReturnStaffName(playerid), reason);
			Log_Write("logs/warn_log.txt", "[%s] %s has warned %s for %s.", ReturnDate(), ReturnStaffName(playerid), ReturnName(userid, 0), reason);
		}
		case 20:
		{
			SendAdminAction(userid, "You've been banned for exceeding your warnings (\"%s\").", reason);
			SendStaffAlert(COLOR_LIGHTRED, "[ADMIN] %s was banned for exceeding a warnings by %s, reason: %s", ReturnName(userid, 0), ReturnName(playerid, 0), reason);
			Log_Write("logs/warn_log.txt", "[%s] %s has warned %s for exceeding a warnings.", ReturnDate(), ReturnStaffName(playerid), ReturnName(userid, 0));

			Blacklist_Add("", PlayerData[playerid][pUsername], "System", reason);
			KickEx(userid);
		}
		default:
		{
			SendAdminAction(userid, "You've been warned by %s for %s", ReturnStaffName(playerid), reason);
			SendStaffAlert(COLOR_LIGHTRED, "[ADMIN] %s was been warned by %s, reason: %s", ReturnName(userid, 0), ReturnStaffName(playerid), reason);
			Log_Write("logs/warn_log.txt", "[%s] %s has warned %s for %s.", ReturnDate(), ReturnStaffName(playerid), ReturnName(userid, 0), reason);
		}
	}
	return 1;
}

